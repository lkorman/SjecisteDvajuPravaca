<Window
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SjecišteDvaPravca"
        xmlns:WPF="clr-namespace:InteractiveDataDisplay.WPF;assembly=InteractiveDataDisplay.WPF" x:Class="SjecišteDvaPravca.Graph"
        mc:Ignorable="d"
        Title="Graph" Height="802" Width="800">
    <Grid x:Name="Grid1">
        <WPF:Chart x:Name="Chart" HorizontalAlignment="Left" VerticalAlignment="Top">
            <Grid x:Name="lines">
                <WPF:CartesianSizeColorMarkerGraph x:Name="CartesianGraph" Margin="0,0,-32,-28" AspectRatio="-4"/>
            </Grid>
        </WPF:Chart>

    </Grid>
</Window>
